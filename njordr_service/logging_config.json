{
    "version": 1,
    "disable_existing_loggers": false,
    "filters": {},
    "formatters": {
        "main": {
            "format": "%(levelname)s|%(created)f|%(filename)s|%(lineno)d|%(message)s"
        }
    },
    "handlers": {
        "stdout": {
            "class": "logging.StreamHandler",
            "formatter": "main",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "DEBUG",
            "handlers": ["stdout"]
        }
    }
}
